@model Automovil
@{
    ViewData["Title"] = "EliminarAuto";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="card">
  <div class="card-header">
    Editar Auto
  </div>
  <div class="card-body">
    
      <form asp-action="EliminarAuto" asp-controller="Mantenedor" method="post">
           <input asp-for="Id" type="hidden" class="form-control" >

         <div class="alert alert-danger" role="alert">
          ¿Esta seguro que quiere borrar este vehiculo? @Html.DisplayTextFor(m => m.Model) ?
        </div>
 
  <button type="submit" class="btn btn-danger">Eliminar</button>
  <a asp-action="ListarAutos" asp-controller="Mantenedor" class="btn btn-warning">Volver a la lista</a>
</form>

  </div>
</div>
